{
  "extends": ["eslint:recommended", "google"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "quotes": [2, "double"],
    "no-console": 1,
    "require-jsdoc": 0,
    "max-len": 0,
    "object-curly-spacing": 0,
    "linebreak-style": 0
  }
}